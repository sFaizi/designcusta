{
	"name": "designcusta",
	"version": "1.0.5",
	"description": "web development and graphic designing",
	"main": "app.js",
	"scripts": {
		"nodemon": "nodemon server.js -e js,scss,pug",
		"watch:sass": "sass --watch sass/main.scss:public/css/style.css --style compressed",
		"compile:js": "watchify ./public/js/index.js -o ./public/js/bundle.js",
		"start": "npm-run-all --parallel nodemon watch:sass compile:js",
		"obfuscate": "javascript-obfuscator ./public/js/bundle.js --output ./public/js/bundle.js",
		"prod": "SET NODE_ENV=production&nodemon server.js"
	},
	"author": "sartaj ahmad faizi",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.48.0",
		"animejs": "^3.2.1",
		"aws-sdk": "^2.1073.0",
		"axios": "^0.22.0",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.19.0",
		"browserify": "^17.0.0",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-mongo-sanitize": "^2.1.0",
		"express-rate-limit": "^5.3.0",
		"helmet": "^5.1.0",
		"html-to-text": "^8.1.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^6.1.4",
		"multer": "^1.4.5-lts.1",
		"multer-s3": "^2.10.0",
		"nodemailer": "^6.7.2",
		"pug": "^3.0.2",
		"slugify": "^1.6.0",
		"uuid": "^8.3.2",
		"validator": "^13.6.0",
		"xss-clean": "^0.1.1"
	},
	"devDependencies": {
		"eslint": "^7.31.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"javascript-obfuscator": "^3.0.0",
		"nodemon": "^2.0.12",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.3.2",
		"sass": "^1.36.0",
		"watchify": "^4.0.0"
	},
	"engines": {
		"node": "18.12.1"
	}
}
